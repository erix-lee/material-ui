    "@types/fs-extra": "^4.0.0",
    "@types/hammerjs": "^2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "^2.52.0",
    "autoprefixer": "7.1.2",
    "codelyzer": "~4.0.0",
    "coveralls": "^2.12.0",
    "fs-extra": "^4.0.0",
    "glob": "^6.0.4",
    "gulp": "3.9.1",
    "gulp-bump": "2.1.0",
    "gulp-help": "1.6.1",
    "gulp-if": "2.0.1",
    "gulp-postcss": "7.0.0",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "3.1.4",
    "gulp-util": "3.0.7",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "1.0.1",
    "merge2": "1.0.2",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "~5.1.0",
    "require-dir": "0.3.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "semver": "5.2.0",
    "ts-node": "^3.0.4",
    "tslint": "5.2.0",
    "typescript": "~2.4.2",
    "uglify-js": "^2.8.14"
  }
}
