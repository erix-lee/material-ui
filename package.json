{
  "name": "material-ui",
  "version": "0.2.0",
  "private": true,
  "description": "Teradata UI Platform built on Angular Material",
  "keywords": [
    "angular",
    "components",
    "reusable"
  ],
  "scripts": {
    "e2e": "protractor",
    "electron": "electron index.js",
    "lint": "tslint --format codeFrame -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
    "test": "ng test --code-coverage --single-run --sourcemap=false",
    "package:mac": "electron-packager .  app --platform=darwin --arch=x64 --icon=hosts.icns --out=./dist/darwin --asar --app-version=2.0.4 --ignore=\"(src|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "package:win": "electron-packager --overwrite .  app --platform=win32 --arch=x64 --icon=src/favicon.ico --out=./dist/win --asar --app-version=2.0.4 --ignore=\"(src|.gitignore|LICENSE|README.md|webpack.config.js|node_modules)\"",
    "package:linux": "electron-packager .  app --platform=linux --arch=x64 --out=./dist/linux --asar --app-version=2.0.4 --ignore=\"(src|.gitignore|LICENSE|README.md|webpack.config.js|node_modules)\"",
    "serve:prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng serve --aot --prod --sourcemap=false --build-optimizer",
    "build:prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --aot --prod --sourcemap=false --build-optimizer"
  },
  "engines": {
    "node": ">=6.11.1 < 7",
    "npm": ">3"
  },
  "license": "MIT",
  "author": "Lee_erix",
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ],
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@angular/material": "^5.0.0-rc.3",
    "@angular/cdk": "^5.0.0-rc.3",
    "@angular/material-moment-adapter": "^5.0.0-rc.3",
    "@covalent/code-editor": "^1.0.0-beta.1",
    "@covalent/core": "^1.0.0-rc.0",
    "@covalent/dynamic-forms": "^1.0.0-rc.0",
    "@covalent/highlight": "^1.0.0-rc.0",
    "@covalent/http": "^1.0.0-rc.0",
    "@covalent/markdown": "^1.0.0-rc.0",
    "@covalent/text-editor": "^1.0.0-alpha.4",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "2.0.0",
    "@swimlane/ngx-charts": "^7.0.1",
    "@swimlane/ngx-datatable": "^11.1.5",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "d3": "^4.4.0",
    "electron": "^1.7.9",
    "emoji-flags": "^1.2.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.11.0",
    "mdi": "^2.0.46",
    "moment": "^2.19.3",
    "node-sass": "^4.7.2",
    "rxjs": "^5.5.2",
    "showdown": "1.6.4",
